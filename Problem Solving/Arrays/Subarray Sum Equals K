Subarray Sum Equals K

class Solution {
    public int subarraySum(int[] nums, int k) {
        HashMap<Integer,Integer>mp=new HashMap<>();
        int presum=0;
        int count=0;
        mp.put(0,1);
        for(int i=0;i<nums.length;i++){
            presum+=nums[i];
            count+=mp.getOrDefault(presum-k,0);
            mp.put(presum,mp.getOrDefault(presum,0)+1);
        }
        return count;
    }
}